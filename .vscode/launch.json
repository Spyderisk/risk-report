{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python Debugger: Current File with Arguments",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": [
                "-i", "input.nq",
                "-o", "output.csv",
                "-d", "domain/csv/",
                "-m", "MS-Behaviour-AssetID",
            ],
            "purpose": ["debug-in-terminal"]
        },
    ]
}